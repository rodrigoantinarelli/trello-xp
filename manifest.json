{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon_browser.png",
      "default_title": "Trello XP"
   },
   "content_scripts": [ {
      "js": [ "jquery.min.js", "content.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "See how many XP you have.",
   "icons": {
      "128": "icon.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Trello XP",
   "permissions": [ "tabs", "http://*/*", "activeTab" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.2"
}
